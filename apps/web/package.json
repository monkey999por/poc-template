    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^4.6.2",
    "install": "^0.13.0",
    "npm": "^11.6.2",
    "tailwindcss": "^4.1.14",
    "typescript": "^5.9.3",
    "vite": "^6.4.0",
    "vite-tsconfig-paths": "^4.3.2",
    "wrangler": "^4.43.0"
  },
  "engines": {
    "node": ">=20.0.0"
  }
